type refdomain = {
  refdomain: string;
  backlinks : int;
  refpages : int;
  firstSeen <json name="first_seen"> : string;
  lastVisited <json name="last_visited"> : string wrap <ocaml t="Js.Date.t"
                              wrap="Js.Date.fromString"
                              unwrap="Js.Date.toString">;
  domainRating <json name="domain_rating"> : int;
}

type response = {
  refdomains: refdomain list <ocaml repr="array">;
}
